  "main": "./dist/server.js",
  "author": "Ryan Smythe",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run tslint && npm run build-server && npm run build-client",
    "run": "node dist/server.js",
    "serve": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TSServer,TSClient,Node,Copy\" -c \"cyan.bold,magenta.bold,green.bold,gray\" \"npm run watch-ts-server\" \"npm run watch-ts-client\" \"npm run serve\" \"npm run watch-copy\"",
    "test": "jest --forceExit",
    "build-server": "gulp build-server",
    "build-client": "gulp build-client",
    "watch-copy": "npm run npm-watch",
    "tslint": "tslint -c tslint.json -p server/src/tsconfig.json && tslint -c tslint.json -p client/src/tsconfig.json",
    "copy": "node copyBuildFiles.js"
  },
  "watch": {
    "copy": {
      "patterns": [
        "server/dist"
      ],
      "extensions": "js,js.map,pug,css,eot,svg,ttf,woff,woff2,otf,webp,jpg,png,gif,env",
      "delay": 2500
    }
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
